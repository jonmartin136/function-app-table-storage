{
  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "authLevel": "Anonymous",
      "methods": [
        "post",
        "get"
      ],
      "direction": "in"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "outputTable",
      "type": "table",
      "tableName": "outtable",
      "connection": "AzureAppTableStorage",
      "direction": "out"
    }
  ]
}
